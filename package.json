{
  "name": "etl-learning",
  "version": "0.1.0",
  "bin": {
    "etl-learning": "bin/etl-learning.js"
  },
  "scripts": {
    "dev": "tsc -w",
    "build:cdk": "node build.js -t cdk",
    "build:functions": "node build.js -t functions",
    "build": "npm run clean && npm run build:cdk && npm run build:functions",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:watch": "jest --config test/jest.config.ts --watch",
    "test:ci": "jest --runInBand --config test/jest.config.ts --detectOpenHandles",
    "clean": "rm -rf dist cdk.out",
    "synth": "npm run build && cdk synth > /dev/null",
    "deploy": "npm run synth && cdk deploy --outputs-file=output.json",
    "bootstrap": "npm run synth && cdk bootstrap",
    "destroy": "cdk destroy",
    "update": "ncu -u"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.81.0",
    "@aws-cdk/aws-apigatewayv2": "1.81.0",
    "@aws-cdk/aws-lambda": "1.81.0",
    "@aws-cdk/aws-stepfunctions": "1.81.0",
    "@aws-cdk/aws-stepfunctions-tasks": "1.81.0",
    "@aws-cdk/core": "1.81.0",
    "@types/app-root-path": "1.2.4",
    "@types/aws-lambda": "8.10.68",
    "@types/jest": "26.0.19",
    "@types/lodash": "4.14.166",
    "@types/node": "14.14.17",
    "@types/stream-to-promise": "2.2.1",
    "app-root-path": "3.0.0",
    "aws-cdk": "1.81.0",
    "chalk": "4.1.0",
    "change-case": "4.1.2",
    "esbuild": "0.8.27",
    "fast-glob": "3.2.4",
    "is-ci": "2.0.0",
    "is-ci-cli": "2.1.2",
    "jest": "26.6.3",
    "msw": "0.24.4",
    "nock": "13.0.5",
    "npm-check-updates": "10.2.5",
    "source-map-support": "0.5.19",
    "stream-to-promise": "3.0.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "yargs": "16.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.1.0",
    "@aws-sdk/client-s3": "3.1.0",
    "@aws-sdk/lib-storage": "3.1.0",
    "@aws-sdk/util-dynamodb": "3.1.0",
    "@expo/results": "1.0.0",
    "aws-sdk": "2.819.0",
    "csvtojson": "2.0.10",
    "lodash": "4.17.20",
    "phin": "3.5.1"
  }
}
